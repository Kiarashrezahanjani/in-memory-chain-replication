//Listening port for protocol server and buffer server.
protocol_port = 3333
buffer_server_port = 4444

//buffer size for each ensemble
ensemble_buffer_size = 100

//zookeeper properties should only be placed in default properties file. 
//The zkNameSpace is the parent nodes and the rest will be child nodes. Nodes should include slash at the beginning.
zkConnectionString = localhost:2181
zkSessionTimeOut = 3000
//logservice will be the root and the rest will be the children
zkNameSpace = /logservice
zkServersRoot = /servers
zkClientRoot = /clients
zkEnsemblesRoot = /ensembles
zkServersGlobalViewRoot = /serversglobalview

//global view update interval in millisecond
gvInfoInterval = 6000

//server + ensemble + gc + check for leadership interval 
serverInfoInterval = 4000
serverInfoIntervalDeviation = 3000

//serverUpdateThreshold indicates the minimum change required in the left capacity to update the server status and capacity
serverUpdateThreshold = 5

//20(percent) means 80 of resources and consumed and server or ensemble should not accept new connections(clients or ensemble request)  
saturationPoint = 20